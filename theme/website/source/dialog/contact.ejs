<md-toolbar>
  <div class="md-toolbar-tools">
    <md-icon class="header-icon" md-svg-src=<%-get_icon("material/message")%>></md-icon>
    <h2><%-lng(page.title)%></h2>
    <span flex></span>
    <md-button class="md-icon-button"  ng-click="close()">
      <md-icon md-svg-src=<%-get_icon("material/close")%> aria-label="Close dialog"></md-icon>
    </md-button>
  </div>
</md-toolbar>
<md-dialog-content>
  <md-tabs  md-selected="tabSelected" md-dynamic-height md-border-bottom>
    <%for(var i in page.tabs){
      var tab = page.tabs[i];%>
      <md-tab  label="<%-lng(tab.label)%>" <%-tab.attrs%>  md-active="isTabActive(<%-i%>)" md-on-select="onTabSelected(<%-i%>)">
      <%-template('dialog/contact/'+tab.name)%>
      </md-tab>
      <%}%>
    </md-tabs>
    <%-template('ui/loader')%>
  </md-dialog-content>
