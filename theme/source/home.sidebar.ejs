<md-list>
<%
var menu = null;
if(has_data('menu')){
  menu = get_data('menu');
}
if(!_.isArray(menu)){
  menu = site.pages.find({
    parent: 'home'
  })
  .map(function(page){
    return {
      page: page.path
    }
  });
}
%>
<%-template('layout/menu/iterator',{itemPath:'sidebar/page-home/item',menu:menu})%>

</md-list>
