<div class="ag-margin md-padding container" layout="column">
<p>
  Click below to select and upload an image from your computer.
</p>

<div layout="column" layout-align="center center">
<md-button flex="1"  type="file" ngf-accept="accept" ngf-select="upload($file)">
    <md-icon md-svg-src="<%-get_icon('material/file-upload')%>" aria-label="upload"></md-icon>
    <span>Upload</span>
</md-button>
</div>
<%-template('ui/loader')%>
</div>
