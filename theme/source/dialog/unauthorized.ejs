<md-toolbar>
  <div class="md-toolbar-tools">
    <md-icon class="header-icon" md-svg-src=<%-get_icon("material/error")%>></md-icon>
    <h2><%-lng(page.title)%></h2>
    <span flex></span>
    <md-button class="md-icon-button" ng-click="close()">
      <md-icon md-svg-src=<%-get_icon("material/close")%> aria-label="Cancel"></md-icon>
    </md-button>
  </div>
</md-toolbar>

<md-dialog-content style="max-width:500px;max-height:600px; ">
  <p class="md-padding">
    <%-lng(page.description)%>
  </p>
</md-dialog-content>
