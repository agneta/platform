<script type="text/ng-template" id="remove-account.html">
    <md-dialog aria-label="Remove Account">
        <md-dialog-content class="md-padding" align="center">
            Are you sure you want to remove this account?
        </md-dialog-content>
        <md-dialog-actions layout="row">
            <span flex></span>
            <md-button ng-click="next(true)">
                Yes
            </md-button>
            <md-button ng-click="cancel()">
                No
            </md-button>
        </md-dialog-actions>
    </md-dialog>
</script>
