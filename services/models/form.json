{
    "name": "Form",
    "base": "PersistedModel",
    "properties": {
        "accountId": {
            "type": "String",
            "required": false
        },
        "language": {
            "type": "String",
            "required": true
        },
        "name": {
            "type": "String",
            "required": false
        },
        "title": {
            "type": "String",
            "required": false
        },
        "data": {
            "type": "object",
            "required": true
        }
    },
    "mixins": {
        "TimeStamp": true
    },
    "relations": {
        "account": {
            "type": "belongsTo",
            "model": "Account",
            "foreignKey": "accountId"
        }
    },
    "acls": [{
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }]
}
